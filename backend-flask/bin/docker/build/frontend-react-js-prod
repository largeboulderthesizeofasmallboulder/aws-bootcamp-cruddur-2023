--build-arg REACT_APP_AWS_USER_POOLS_ID="us-east-1_PaCaWCGSC" \
--build-arg REACT_APP_CLIENT_ID="19kfkj5dv37e7blnvl5rc8qvl6" \
-t frontend-react-js \
-f Dockerfile.prod \
.